<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
	id="WebApp_ID" version="6.0">
	<display-name>syLibrary</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>login.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MemberController</servlet-name>
		<servlet-class>member.MemberController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MemberController</servlet-name>
		<url-pattern>/member_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AdminController</servlet-name>
		<servlet-class>admin.login.AdminController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminController</servlet-name>
		<url-pattern>/admin_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AdmemberController</servlet-name>
		<servlet-class>member.AdmemberController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdmemberController</servlet-name>
		<url-pattern>/admember_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SearchController</servlet-name>
		<servlet-class>bookSearch.SearchController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchController</servlet-name>
		<url-pattern>/search_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>BookController</servlet-name>
		<servlet-class>admin.book.BookController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BookController</servlet-name>
		<url-pattern>/book_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoanBookController</servlet-name>
		<servlet-class>admin.l_book.LoanBookController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoanBookController</servlet-name>
		<url-pattern>/loanBook_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReBookController</servlet-name>
		<servlet-class>admin.l_book.ReBookController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReBookController</servlet-name>
		<url-pattern>/rebook_servlet/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyLibraryController</servlet-name>
		<servlet-class>myLibrary.MyLibraryController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyLibraryController</servlet-name>
		<url-pattern>/myLibrary_servlet/*</url-pattern>
	</servlet-mapping>
</web-app>