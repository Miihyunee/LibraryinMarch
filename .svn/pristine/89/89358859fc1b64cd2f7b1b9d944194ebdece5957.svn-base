<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">

	<select id="list_memberInfo" resultType="member.MemberDTO">
	<!-- 회원정보 조회 -->
		SELECT * FROM sl_member
	</select>

	<update id="edit_memberInfo">
	<!-- 회원정보 수정 -->
		UPDATE SET m_passwd = #{m_passwd}, m_name =
		#{m_name}, m_tel = #{m_tel}, m_email = #{m_email}
		m_address =
		#{m_address}
		FROM sl_member
		WHERE m_no = #{m_no}
	</update>

	<select id="ad_list" resultType="member.MemberDTO">
		<!-- 관리자회원전체리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
	</select>
	
	<select id="ad_detail" resultType="member.MemberDTO">
		<!-- 관리자회원상세 -->
		select m_name, m_id, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date, m_tel, m_email, m_address, m_url,to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
	</select>
	
	<select id="ad_name_list" resultType="member.MemberDTO">
		<!-- 관리자회원이름검색리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
		where m_name like '%'|| #{search} ||'%'
		order by m_name
	
	</select>
	
	<select id="ad_birth_list" resultType="member.MemberDTO">
		<!-- 관리자회원생년월일검색리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
		where m_birth_date like '%'|| #{search} ||'%'
		order by m_birth_date
	
	</select>
	
	<select id="ad_id_list" resultType="member.MemberDTO">
		<!-- 관리자회원아이디검색리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
		where m_id like '%'|| #{search} ||'%'
		order by m_id
	
	</select>
	
		<select id="ad_tel_list" resultType="member.MemberDTO">
		<!-- 관리자회원전화번호검색리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
		where m_tel like '%'|| #{search} ||'%'
		order by m_name
	
	</select>
	
	<select id="ad_year_list" resultType="member.MemberDTO">
		<!-- 관리자회원가입일검색리스트 -->
		select m_name, to_char(m_birth_date, 'yyyy-mm-dd') m_birth_date , m_id, m_tel, to_char(m_year, 'yyyy-mm-dd') m_year
		from sl_member
		where m_year like '%'|| #{search} ||'%'
		order by m_name
	
	</select>
	

</mapper>